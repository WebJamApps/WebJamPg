{
  "name": "webjampg",
  "version": "0.0.1",
  "engines": {
    "node": ">=14.4.0"
  },
  "main": "src/index",
  "files": [
    "src/"
  ],
  "scripts": {
    "docker:up": "docker-compose -f docker-compose.yml up --build",
    "docker:pull": "yarn docker:down && docker pull node:current-alpine && docker-compose pull pgadmin && yarn docker:up",
    "docker:down": "docker-compose -f 'docker-compose.yml' down",
    "cc": "codeclimate analyze -f html > codeclimate.html",
    "installglobals": "npm install npm@latest -g && npm install snyk -g",
    "cleaninstall": "rm -rf yarn.lock && rm -rf package-lock.json && rm -rf node_modules && yarn cache clean && yarn run installglobals && yarn install",
    "start:debug": "rm -rf ./build && tsc && concurrently \"yarn ts-start\" \"tsc -w\"",
    "ts-start": "DEBUG=webjampg:* nodemon build/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "node src/index.js",
    "test": "eslint . --debug && yarn test:jest",
    "test:jest": "jest test --no-cache --config jest.config.json --runInBand --notify --detectOpenHandles --forceExit",
    "test:local": "eslint . --fix && yarn test:jest && yarn cc",
    "test:watch": "jest . --watch",
    "test:snyk": "snyk test"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.7",
    "@types/express-fileupload": "^1.1.3",
    "@types/helmet": "^0.0.47",
    "@types/morgan": "^1.9.1",
    "bcryptjs": "latest",
    "body-parser": "latest",
    "cors": "latest",
    "debug": "latest",
    "dotenv": "latest",
    "express": "latest",
    "express-fileupload": "latest",
    "fs-extra": "latest",
    "helmet": "latest",
    "jimp": "latest",
    "morgan": "latest",
    "pg": "^8.3.0",
    "pg-hstore": "latest",
    "sequelize": "latest",
    "statsd-client": "latest"
  },
  "devDependencies": {
    "@types/jest": "^26.0.5",
    "@types/sinon": "^9.0.4",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "concurrently": "^5.2.0",
    "eslint": "latest",
    "eslint-config-airbnb": "latest",
    "eslint-config-airbnb-typescript": "latest",
    "eslint-plugin-es": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jest": "latest",
    "eslint-plugin-json": "latest",
    "eslint-plugin-jsx-a11y": "latest",
    "eslint-plugin-node": "latest",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "latest",
    "eslint-plugin-security": "latest",
    "jest": "latest",
    "nodemon": "latest",
    "sinon": "latest",
    "snyk": "latest",
    "supertest": "latest",
    "ts-jest": "^26.1.3",
    "typescript": "latest"
  }
}
