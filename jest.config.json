 {
   "bail": true,
   "clearMocks": true,
   "collectCoverage": true,
   "collectCoverageFrom": ["src/**/*.{js,jsx}"],
   "coverageDirectory": "coverage",
   "coveragePathIgnorePatterns": ["/node_modules/", "/front-end/"],
   "testPathIgnorePatterns": ["/node_modules/", "<rootDir>/front-end/tests/", "<rootDir>/test/__mocks__/"],
   "verbose": true,
   "testEnvironment": "node",
   "roots": ["<rootDir>/src/", "<rootDir>/test"]
 }
